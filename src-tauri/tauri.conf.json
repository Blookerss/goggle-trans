{
    "package": {
        "productName": "goggle trans",
        "version": "2.2.1"
    },
    "build": {
        "distDir": "../build",
        "devPath": "http://localhost:3000",
        "withGlobalTauri": true,
        "beforeDevCommand": "",
        "beforeBuildCommand": ""
    },
    "tauri": {
        "bundle": {
            "active": true,
            "targets": "all",
            "identifier": "io.github.blookers.goggletrans",
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "resources": [],
            "externalBin": [],
            "copyright": "",
            "category": "Utility",
            "shortDescription": "translation, 1000% funnier.",
            "longDescription": "goggle trans, messing up sentences for your own enjoyment.",
            "deb": {
                "depends": [],
                "useBootstrapper": false
            },
            "macOS": {
                "frameworks": [],
                "minimumSystemVersion": "",
                "useBootstrapper": false,
                "exceptionDomain": "",
                "signingIdentity": null,
                "entitlements": null
            },
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256",
                "timestampUrl": "",
                "wix": {
                    "dialogImagePath": "icons/installer_bg.png",
                    "bannerPath": "icons/installer_banner.png"
                }
            }
        },
        "updater": {
            "active": false
        },
        "allowlist": {
            "notification": {
                "all": true
            },
            "window": {
                "all": true,
                "create": true
            },
            "fs": {
                "all": true,
                "readTextFile": true,
                "readBinaryFile": true,
                "writeFile": true,
                "writeBinaryFile": true,
                "readDir": true,
                "copyFile": true,
                "createDir": true,
                "removeDir": true,
                "removeFile": true,
                "renameFile": true
            },
            "path": {
                "all": true
            },
            "http": {
                "all": true,
                "request": true
            }
        },
        "windows": [{
            "title": "goggle trans",
            "width": 800,
            "height": 500,
            "center": true,
            "focus": true,
            "resizable": true,
            "fullscreen": false,
            "decorations": false,
            "transparent": true
        }],
        "security": {
            "csp": "default-src blob: data: filesystem: ws: http: https: * 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
        }
    }
}