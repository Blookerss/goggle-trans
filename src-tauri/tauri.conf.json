{
    "package": {
        "productName": "goggle trans",
        "version": "2.3.0"
    },
    "build": {
        "distDir": "../build",
        "devPath": "http://localhost:3000",
        "beforeDevCommand": "",
        "beforeBuildCommand": ""
    },
    "tauri": {
        "bundle": {
            "active": true,
            "targets": "all",
            "identifier": "io.github.blookers.goggletrans",
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "resources": [],
            "externalBin": [],
            "copyright": "",
            "category": "Utility",
            "shortDescription": "translation, 1000% funnier.",
            "longDescription": "goggle trans, messing up sentences for your own enjoyment.",
            "deb": {
                "depends": []
            },
            "macOS": {
                "frameworks": [],
                "minimumSystemVersion": "",
                "exceptionDomain": "",
                "signingIdentity": null,
                "entitlements": null
            },
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256",
                "timestampUrl": "",
                "wix": {
                    "dialogImagePath": "icons/installer_bg.png",
                    "bannerPath": "icons/installer_banner.png"
                }
            }
        },
        "updater": {
            "active": false,
            "dialog": true,
            "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDk3RjcyQTM5ODVGMzhCQzUKUldURmkvT0ZPU3IzbDdkcERoenIra1Mvc0tmZjFsbFYxTWszcEJyWVRJOWg2eVNpRG5RRmJZRDMK",
            "endpoints": [
                "https://voxelified.com/api/v1/releases/update/goggle-trans/{{target}}/{{current_version}}"
            ]
        },
        "allowlist": {
            "all": false,
            "path": {
                "all": true
            },
            "http": {
                "request": true
            },
            "window": {
                "close": true,
                "minimize": true,
                "maximize": true,
                "unmaximize": true,
                "startDragging": true
            },
            "clipboard": {
                "writeText": true
            }
        },
        "windows": [{
            "title": "goggle trans",
            "width": 800,
            "height": 500,
            "center": true,
            "focus": true,
            "resizable": true,
            "fullscreen": false,
            "decorations": false,
            "transparent": true
        }],
        "security": {
            "csp": "default-src blob: data: filesystem: ws: http: https: * 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
        }
    }
}